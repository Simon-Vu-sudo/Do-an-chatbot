{
  "compilerOptions": {
    "jsx": "react-native",
    "module": "esnext",
    "moduleResolution": "bundler",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "strict": true,
    "noEmit": true,
    "resolveJsonModule": true
  },
  "include": ["src"],
  "extends": "expo/tsconfig.base"
}
